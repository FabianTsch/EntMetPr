<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="PickPlace">
    <Code><![CDATA[begin
  
  
  while true
    
    wait(bNewData)
    
    if bPick
      call pick(nX,nY)
    else
      call place(nX,nY)
    endIf
    
    
    bNewData = false
    bReady = true
    
    
    
  endWhile
  
end]]></Code>
  </Program>
</Programs>